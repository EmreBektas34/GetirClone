<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GetirClone.App">
    <Application.Resources>
<ResourceDictionary>
          <OnPlatform x:TypeArguments="x:String" x:Key="fontUI">
             <On Platform="Android" Value="fontUI.otf#fontUI" />
             <On Platform="UWP" Value="/Assets/fontUI.otf#fontUI" />
             <On Platform="iOS" Value="fontUI" />
          </OnPlatform>
       </ResourceDictionary>
    </Application.Resources>
</Application>